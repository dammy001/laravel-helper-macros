{
    "name": "damilare/laravel-helper-macros",
    "description": "Laravel Helper Macros",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Damilare Anjorin",
            "email": "damilareanjorin1@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/contracts": "11.1.0",
        "illuminate/support": "11.1.0"
    },
    "require-dev": {
        "orchestra/testbench": "8.21.1",
        "phpunit/phpunit": "10.5.15"
    },
    "autoload": {
        "psr-4": {
            "Damilare\\LaravelHelperMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Damilare\\LaravelHelperMacros\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Damilare\\LaravelHelperMacros\\HelperMacroServiceProvider"
            ]
        }
    }
}
